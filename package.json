{
  "name": "rejoes-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:dev": "prisma migrate dev",
    "test": "npx cross-env NODE_ENV=test DATABASE_URL=\"file:./test/test.db\" SHOPIFY_WEBHOOK_SECRET=testsecret prisma migrate deploy && npx cross-env NODE_ENV=test DATABASE_URL=\"file:./test/test.db\" SHOPIFY_WEBHOOK_SECRET=testsecret vitest run"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.19",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "cross-env": "^7.0.3",
    "prisma": "^5.10.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0"
  }
}
